#pink .header{background: #fff;} 
#pink .main{background: #fff;}
#pink .m-block{border-bottom:1rpx solid #D8D8D8;color: #545454;}
#pink .getUser{background: #FF5E71;}
#pink .dark{display: none;}
#pink .userinfo-nickname{color: #333333;}
#pink .m-block:last-child{border: none;}

